{
  "name": "koapache",
  "version": "1.0.4",
  "description": "A Web server which is easy to use in command-line or as a forked Child process based on Koa",
  "keywords": [
    "Web",
    "server",
    "command-line",
    "worker",
    "koa",
    "apache"
  ],
  "author": "shiy2008@gmail.com",
  "license": "LGPL-3.0",
  "homepage": "https://techquery.github.io/KoApache/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TechQuery/KoApache.git"
  },
  "bugs": {
    "url": "https://github.com/TechQuery/KoApache/issues"
  },
  "main": "dist/koapache.js",
  "module": "source/WebServer.js",
  "bin": {
    "web-server": "dist/koapache-cli.js"
  },
  "engines": {
    "node": "^5.12.0"
  },
  "scripts": {
    "lint": "eslint source/ test/ --fix",
    "pack-command": "amd-bundle source/command dist/koapache-cli",
    "pack-module": "amd-bundle source/WebServer dist/koapache",
    "build": "npm run lint  &&  npm run pack-command  &&  npm run pack-module",
    "test": "mocha --exit",
    "debug": "mocha --inspect-brk",
    "prepublishOnly": "npm run build  &&  npm test",
    "precommit": "npm run prepublishOnly  &&  esdoc  &&  git add .",
    "start": "node dist/koapache-cli docs/ -o"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@koa/cors": "^2.2.2",
    "@tech_query/node-toolkit": "^0.6.0",
    "commander": "^2.18.0",
    "form-data": "^2.3.2",
    "internal-ip": "^3.0.1",
    "koa": "^2.5.3",
    "koa-body": "^4.0.4",
    "koa-logger": "^3.2.0",
    "koa-static": "^5.0.0",
    "opn": "^5.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "amd-bundle": "^1.6.0",
    "esdoc": "^1.1.0",
    "esdoc-external-nodejs-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.6.0",
    "husky": "^0.14.3",
    "mocha": "^5.2.0",
    "should": "^13.2.3",
    "should-http": "^0.1.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "koapache": {
    "proxy": {
      "^/github/(.+)": "https://api.github.com/$1"
    }
  }
}
