{
  "name": "koapache",
  "version": "0.4.0",
  "description": "A Web server which is easy to use in command-line or as a forked Child process based on Koa",
  "main": "source/WebServer.js",
  "bin": {
    "web-server": "dist/command.js"
  },
  "engines": {
    "node": "^5.12.0"
  },
  "scripts": {
    "lint": "eslint source/ test/ --fix  &&  git add .",
    "build": "amd-bundle source/command dist/command -c  &&  esdoc  &&  git add .",
    "prepublishOnly": "npm run build",
    "start": "node dist/command docs/ -o",
    "check": "mocha -r babel-register -r should -r should-http -c --inline-diffs",
    "test": "npm run check -- --exit",
    "debug": "npm run check -- --inspect-brk",
    "precommit": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TechQuery/KoApache.git"
  },
  "keywords": [
    "Web",
    "server",
    "command-line",
    "worker",
    "koa",
    "apache"
  ],
  "author": "shiy2008@gmail.com",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/TechQuery/KoApache/issues"
  },
  "homepage": "https://techquery.github.io/KoApache/",
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "commander": "^2.16.0",
    "internal-ip": "^3.0.1",
    "koa": "^2.5.1",
    "koa-logger": "^3.2.0",
    "koa-static": "^4.0.3",
    "node-fetch": "^2.1.2",
    "opn": "^5.3.0"
  },
  "devDependencies": {
    "amd-bundle": "^1.3.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "esdoc": "^1.1.0",
    "esdoc-external-nodejs-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.19.1",
    "husky": "^0.14.3",
    "mocha": "^5.2.0",
    "should": "^13.2.1",
    "should-http": "^0.1.1"
  },
  "koapache": {
    "proxy": {
      "^/github/(.+)": "https://api.github.com/$1"
    }
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false
        }
      ]
    ]
  }
}
